{
  "name": "@melodysdad/pm-models",
  "version": "1.0.1",
  "description": "Generated TypeScript types, schemas, and utilities for project manager domain models",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/melodysdad/project_manager_models.git",
    "directory": "packages/models"
  },
  "scripts": {
    "build": "tsc",
    "prepublish": "npm run build"
  },
  "keywords": [
    "project-manager",
    "models",
    "types",
    "schemas",
    "zod"
  ],
  "dependencies": {
    "zod": "^3.22.0"
  }
}
